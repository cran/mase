citHeader("To cite mase in publications use:")

bibentry(bibtype = "Manual",
  title        = "mase: Model-Assisted Survey Estimation",
  author       = c(as.person("Kelly McConville"), as.person("Josh Yamamoto"),
                   as.person("Becky Tang"), as.person("George Zhu"), as.person("Shirley Cheung"), as.person("Sida Li")),
  year         = "2018",
  url          = "https://cran.r-project.org/package=mase", 
  textVersion = 
  paste("Kelly McConville, Josh Yamamoto Becky Tang, George Zhu, Shirley Cheung, and Sida Li (2018).",
        "mase: Model-Assisted Survey Estimation.",
        "R package version 0.1.4",
        "https://cran.r-project.org/package=mase")
)
